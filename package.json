{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "dev:host": "vite --host",
        "build:production": "vite build --mode production",
        "preview": "vite preview",
        "watch": "vite build --watch",
        "clean": "rm -rf public/build",
        "format": "prettier --write --plugin=prettier-plugin-solidity ./resources/\"**/*.{js,jsx,ts,tsx,vue,html,css,scss,json,md,sol}\""
    },
    "devDependencies": {
        "autoprefixer": "^10.4.22",
        "axios": "^1.13.2",
        "concurrently": "^9.0.1",
        "prettier-plugin-solidity": "^2.2.0",
        "laravel-vite-plugin": "^2.0.0",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.18",
        "vite": "^7.0.7"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^7.1.0",
        "tom-select": "^2.4.3",
        "ziggy-js": "^2.6.0"
    }
}
